  #include<iostream>
using namespace std;



struct Point{
    float x;
    float y;
};
int main (){
    Point points[7];
    cout << "Enter coordinates of all (x,y)\n";
    for(int i=0;i<7;i++){
        cout << "Points " << i+1 <<": ";
        cin>>points[i].x>>points[i].y;
    }
    int count = 0;
    for(int i=0;i<7;i++){
        if(points[i].x>0&&points[i].y>0){
            count++;
        }
    }
    cout << "\n Number of points in the first quadrant :" << count << endl;
    return 0;
}
